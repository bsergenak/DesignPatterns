@model DesignPattern.CQRS.CQRSPattern.Commands.CreateProductCommand
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<div class="container">
    <br />
    <form method="post">
        <label>Urun adi</label>
        <input type="text" class="form-control" asp-for="Name" />
        <br />

        <label>Urun Stok</label>
        <input type="text" class="form-control" asp-for="Stock" />
        <br />

        <label>Urun Fiyat</label>
        <input type="text" class="form-control" asp-for="Price" />
        <br />

        <label>Urun aciklama</label>
        <input type="text" class="form-control" asp-for="Description" />
        <br />
        <button class="btn btn-dark">Urun ekle</button>
    </form>
</div>

